{
    "FIX": {
        "prefix": [
            "FIX",
            "fix"
        ],
        "body": [
            "$LINE_COMMENT FIX: ${VIM:system('git config --get user.name 2>/dev/null | tr -d \"\n\"')} $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
            "$LINE_COMMENT $0"
        ],
