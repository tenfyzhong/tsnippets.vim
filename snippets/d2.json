{
    "shape": {
        "prefix": "shape",
        "body": [
            "shape: ${1:rectangle}"
        ],
        "description": "shape of the key"
    },
    "exp": {
        "prefix": "exp",
        "body": [
            "explanation: |${1:md}",
            "  $2",
            "| ${3:{",
            "  near: $4",
            "}}"
        ],
        "description": "explanation text or code"
    },
    "icon": {
        "prefix": "icon",
        "body": [
            "icon: ${1:https://icons.terrastruct.com/infra/019-network.svg}"
        ],
        "description": "icon with url"
    },
    "img": {
        "prefix": "img",
        "body": [
            "${1:key}: {",
            "  shape: image",
            "  icon: ${2:https://icons.terrastruct.com/dev/github.svg}",
            "}"
        ],
        "description": "image"
    },
    "sql": {
        "prefix": "sql",
        "body": [
            "${1:table}: {",
            "  shape: sql_table",
            "  id: int {constraint: primary_key}",
            "  $0",
            "}"
        ],
        "description": "sql_table"
    },
    "pk": {
        "prefix": "pk",
        "body": [
            "{constraint: primary_key}"
        ],
        "description": "primary_key for a table constraint"
    },
